<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= simple_form_for(@field) do |f| %>
      <%= f.error_notification %>
      <%= f.input :name %>
      <%= f.input :color %>
      <%= f.input :position %>
      <%= f.input :icon %>
      <%= f.association :parent %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>
