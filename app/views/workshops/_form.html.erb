<%= simple_form_for(@workshop) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :url %>
    <%= f.input :dateDebut %>
    <%= f.input :dateFin %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
